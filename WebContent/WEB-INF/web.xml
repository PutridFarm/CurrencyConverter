<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>CurrencyConverter</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>CurrencyConverter</display-name>
    <servlet-name>CurrencyConverter</servlet-name>
    <jsp-file>/index.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>CurrencyConverter</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CurrencyConverterServlet</display-name>
    <servlet-name>CurrencyConverterServlet</servlet-name>
    <servlet-class>com.fdmgroup.CurrencyConverterProject.Servlets.CurrencyConverterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CurrencyConverterServlet</servlet-name>
    <url-pattern>/CurrencyConverterServlet</url-pattern>
  </servlet-mapping>
</web-app>